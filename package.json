{
  "name": "autoflip",
  "version": "1.0.0",
  "description": "automation for flip mobile projects",
  "main": "index.js",
  "repository": "https://github/moxspoy/autoflip",
  "author": "M Nurilman Baehaqi",
  "license": "MIT",
  "private": false,
  "type": "module",
  "scripts": {
    "staging_release_and_debug_slack": "yarn clean_android_production && yarn build_android_staging_debug && yarn build_android_staging_release && yarn move_to_work_directory && node index.js",
    "staging_release_and_debug_firebase": "yarn clean_android_production && yarn build_android_staging_debug && yarn build_android_staging_release && yarn move_to_work_directory && node index.js",
    "move_to_android_project": "node src/utils/acceptor.js --action move_to_android_project",
    "move_to_flip_project": "node src/utils/acceptor.js --action move_to_flip_project",
    "move_to_work_directory": "node src/utils/acceptor.js --action move_to_work_directory",
    "build_android_bundle_production_release": "yarn move_to_flip_project && ENVFILE=.env.production ./gradlew bundleProductionRelease",
    "build_android_bundle_debug": "yarn move_to_flip_project && ENVFILE=.env.production ./gradlew bundleProductionDebug",
    "build_android_production_release": "yarn move_to_flip_project && ENVFILE=.env.production ./gradlew assembleProductionRelease",
    "build_android_production_debug": "yarn move_to_flip_project && ENVFILE=.env.production ./gradlew assembleProductionDebug",
    "build_android_staging_release": "yarn move_to_flip_project && ENVFILE=.env.staging ./gradlew assembleStagingRelease",
    "build_android_staging_debug": "yarn move_to_flip_project && ENVFILE=.env.staging ./gradlew assembleStagingDebug",
    "clean_android_production": "yarn move_to_flip_project && ENVFILE=.env.production ./gradlew clean"
  },
  "dependencies": {
    "@slack/web-api": "^6.2.4",
    "@slack/webhook": "^6.0.0",
    "dotenv": "^10.0.0",
    "minimist": "^1.2.5"
  }
}
